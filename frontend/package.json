{
  "name": "matt-whitt-website",
  "version": "1.0.0",
  "description": "Matt Whitt's Website",
  "repository": {
    "type": "git",
    "url": "https://github.com/mattrwh/spaceship"
  },
  "homepage": "https://github.com/mattrwh/spaceship",
  "dependencies": {
    "@types/lodash": "^4.14.43",
    "@types/uglify-js": "^2.6.28",
    "bunyan": "^1.3.5",
    "clean-webpack-plugin": "^0.1.3",
    "css-loader": "^0.19.0",
    "extract-text-webpack-plugin": "^2.0.0-beta",
    "file-loader": "^0.8.1",
    "fs-extra": "^0.24.0",
    "http-proxy": "^1.11.1",
    "json-loader": "^0.5.2",
    "lodash": "^4.17.2",
    "minimist": "^1.1.1",
    "moment": "^2.10.6",
    "node-sass": "^3.4.2",
    "npm-run-all": "^1.2.13",
    "postcss-loader": "^0.9.1",
    "pretty-error": "^1.1.2",
    "print-error": "^0.1.8",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-isomorphic-render": "^7.1.2",
    "react-redux": "^4.0.0",
    "react-router": "^2.4.1",
    "redux": "^3.0.0",
    "sass-loader": "^3.1.2",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.12.4",
    "typescript": "^2.1.4",
    "universal-log": "^0.3.10",
    "universal-webpack": "^0.1.31",
    "url-loader": "^0.5.5",
    "web-service": "^0.2.0",
    "webpack": "^2.1.0-beta.25"
  },
  "devDependencies": {
    "@types/node": "0.0.2",
    "awesome-typescript-loader": "^3.0.0-beta.10",
    "express": "^4.13.3",
    "nodemon": "^1.8.0",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.4",
    "redbox-react": "1.2.6",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "typings": "^2.0.0",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.5.1"
  },
  "scripts": {
    "start": "npm run dev",
    "compile-webpack": "tsc -p ./configs/tsconfig.json",
    "dev": "npm-run-all development-prepare-server-build development",
    "development": "npm run compile-webpack && npm-run-all --parallel development-build-client development-build-server development-server",
    "development-prepare-server-build": "universal-webpack --settings ./configs/compiled/webpack/universal-webpack-settings.js prepare",
    "development-build-client": "node ./configs/compiled/webpack/webpack-dev-server.entry.js",
    "development-build-server": "webpack --config ./configs/compiled/webpack/webpack.config.server.development.entry.js --watch --colors --display-error-details",
    "development-page-server": "nodemon ./code/page-server/entry.js --watch ./code/page-server --watch ./code/common --watch ./build/server",
    "development-page-server-delayed": "npm-run-all delay development-page-server",
    "development-web-server": "nodemon ./code/web-server/entry.js --watch ./code/web-server --watch ./code/common",
    "development-web-server-delayed": "npm-run-all delay delay development-web-server",
    "development-server": "npm-run-all --parallel development-page-server-delayed development-web-server-delayed",
    "delay": "node ./code/sleep 500",
    "production": "npm-run-all production-build-client production-build-server production-server",
    "production-build-client": "webpack --colors --display-error-details --config ./configs/compiled/webpack/webpack.config.client.production.entry.js",
    "production-build-server": "webpack --colors --display-error-details --config ./configs/compiled/webpack/webpack.config.server.production.entry.js",
    "production-page-server": "node ./code/page-server/entry.js",
    "production-web-server": "node ./code/web-server/entry.js --production",
    "production-server": "npm-run-all --parallel production-page-server production-web-server"
  },
  "author": "Matthew Whitt",
  "engines": {
    "node": "4.1.1"
  }
}
